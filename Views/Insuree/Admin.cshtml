
@model CarInsurance.Models.Insuree

@{
    ViewBag.Title = "Admin View";
}

<h2>Admin View - Insurance Quotes</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Quote Amount</th>
    </tr>
    @foreach (var insuree in Model)
    {
        <tr>
            <td>@insuree.FirstName</td>
            <td>@insuree.LastName</td>
            <td>@insuree.Email</td>
            <td>@insuree.Quote.ToString("C")</td>
        </tr>
    }
</table>


<h2>Admin</h2>

